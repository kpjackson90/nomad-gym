{
  "name": "nomad-gym",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "11.12.0",
    "npm": "6.13.0"
  },
  "main": "index.js",
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "./dist"
    ],
    "coverageReporters": [
      "lcov",
      "html"
    ],
    "moduleNameMapper": {
      "^mongoose$": "<rootDir>/node_modules/mongoose"
    },
    "setupFilesAfterEnv": [
      "./server/tests/config/databaseConnection.js"
    ]
  },
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js --ignore client",
    "clean": "rimraf build",
    "build": "npm run clean && webpack",
    "serve": "webpack-dev-server --port $PORT --host 0.0.0.0",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install && npm run build",
    "test": "jest --coverage --forceExit --detectOpenHandles",
    "test:watch": "jest --watch --coverage --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "git@github-kpjackson:kpjackson90/nomad-gym.git"
  },
  "author": "Kareem Phillip-Jackson",
  "license": "ISC",
  "dependencies": {
    "apollo-client": "^0.8.2",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.18.0",
    "css-loader": "^3.2.0",
    "express": "^4.17.3",
    "express-graphql": "^0.9.0",
    "graphql": "^14.5.4",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "mongoose": "^5.6.12",
    "nodemon": "^1.19.2",
    "react": "^15.4.2",
    "react-apollo": "^0.10.0",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "react-router-dom": "^5.1.2",
    "rimraf": "^3.0.0",
    "style-loader": "^1.0.0",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.10.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.0.6",
    "jest": "^25.1.0",
    "jest-cli": "^25.1.0",
    "mongodb-memory-server": "^6.2.4",
    "supertest": "^4.0.2"
  }
}
